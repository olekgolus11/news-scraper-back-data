{
    "name": "news-scraper-back",
    "version": "1.0.0",
    "main": "app.ts",
    "scripts": {
        "build": "npm install && npm run build-ts --silent",
        "build-ts": "tsc",
        "dev": "concurrently \"npx tsc --watch\" \"node --env-file=.env --watch lib/app.js\"",
        "start": "node --env-file=.env lib/app.js"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "express": "^4.18.3",
        "http-status-codes": "^2.3.0",
        "mongodb": "^6.4.0",
        "mongoose": "^8.2.0",
        "newsapi": "^2.4.1",
        "ts-md5": "^1.3.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "concurrently": "^8.2.2",
        "typescript": "^5.3.3"
    }
}
